{
    "name": "fbf/laravel-food",
    "description": "A Laravel 4 Package for websites for manufacturers of food based products, with multiple product categories, products, recipes and stockists",
    "license": "MIT",
    "authors": [
        {
            "name": "Neil Crookes",
            "email": "neil.crookes@fivebyfiveuk.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.*",
        "cviebrock/eloquent-sluggable": "1.0.*"
    },
    "autoload": {
        "classmap": [
            "src/composers",
            "src/controllers",
            "src/migrations",
            "src/models"
        ],
        "psr-0": {
            "Fbf\\LaravelFood\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
